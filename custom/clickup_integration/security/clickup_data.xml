<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.model.access" id="clickup_data_access">
        <field name="name">Clickup data access</field>
        <field name="model_id" ref="model_clickup_data"/>
        <field name="group_id" ref="base.group_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

    <record model="ir.model.access" id="clickup_config_access">
        <field name="name">Clickup config access</field>
        <field name="model_id" ref="model_clickup_config"/>
        <field name="group_id" ref="base.group_user"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

</odoo>
